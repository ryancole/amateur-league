{
  "name": "league",
  "version": 2,
  "builds": [{ "src": "api/lambdas/team/*.js", "use": "@now/node" }],
  "routes": [
    {
      "src": "/team",
      "dest": "/api/lambdas/team/get-all.js",
      "methods": ["GET"]
    },
    {
      "src": "/team/(?<id>\\d+)",
      "dest": "/api/lambdas/team/get-single.js?id=$id",
      "methods": ["GET"]
    }
  ]
}
